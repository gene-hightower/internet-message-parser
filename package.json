{
  "name": "message-parser",
  "version": "1.0.0",
  "description": "Parse various things in an RFC-5322 Internet Message",
  "main": "./dist/Message.js",
  "types": "./dist/Message.d.ts",
  "scripts": {
    "prettier": "node node_modules/prettier/bin-prettier.js --write .",
    "peggy": "node node_modules/peggy/bin/peggy --plugin ./node_modules/ts-pegjs/src/tspegjs.js --cache --allowed-start-rules field -o lib/message-parser.ts lib/message-parser.pegjs",
    "build": "tsc",
    "clean": "rm -rf node_modules lib/grammar.ts dist",
    "coverage": "jest --collectCoverage -- dist/test/*.test.js",
    "prepare": "npm run peggy && npm run build",
    "test": "jest dist/test/*.test.js"
  },
  "engines": {
    "node": ">=0.10"
  },
  "keywords": [
    "RFC-5322",
    "email",
    "rfc2822",
    "rfc5322",
    "rfc822"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gene-hightower/message-parser.git"
  },
  "author": "Gene Hightower <gene@digilicious.com> (https://digilicious.com/)",
  "license": "MIT",
  "homepage": "https://github.com/gene-hightower/message-parser#readme",
  "dependencies": {
    "iconv": "^3.0.1",
    "libqp": "^1.1.0",
    "re2-latin1": "^1.17.4",
    "ts-pegjs": "^1.2.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.33",
    "peggy": "^1.2.0",
    "eslint": "*",
    "jest": "^26.6.3",
    "prettier": "2.5.1",
    "typescript": "^4.2.3"
  }
}
